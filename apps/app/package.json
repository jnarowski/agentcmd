{
  "name": "agentcmd",
  "version": "1.1.38",
  "type": "module",
  "main": "dist/server/index.js",
  "bin": {
    "agentcmd": "./dist/cli.js"
  },
  "files": [
    "dist/"
  ],
  "preferGlobal": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/jnarowski/agentcmd"
  },
  "homepage": "https://agentcmd.dev",
  "bugs": {
    "url": "https://github.com/jnarowski/agentcmd/issues"
  },
  "scripts": {
    "build": "pnpm prisma:generate && vite build && pnpm build:server && pnpm build:cli",
    "build:server": "node scripts/build-server.js",
    "build:cli": "node scripts/build-cli.js",
    "check": "pnpm lint && pnpm check-types && pnpm test",
    "cli:install": "DATABASE_URL=file:$HOME/.agentcmd/database.db node dist/cli.js install",
    "cli:start": "DATABASE_URL=file:$HOME/.agentcmd/database.db node dist/cli.js start",
    "check-types": "tsc --noEmit -p tsconfig.app.json && tsc --noEmit -p tsconfig.server.json && tsc --noEmit -p tsconfig.shared.json && tsc --noEmit -p tsconfig.cli.json && tsc --noEmit -p tsconfig.node.json",
    "dev": "prisma migrate deploy && concurrently --kill-others \"pnpm inngest\" \"pnpm dev:server\" \"pnpm dev:client\"",
    "dev:client": "vite --host --open",
    "dev:kill": "bash ./scripts/kill-ports.sh",
    "dev:setup": "tsx scripts/setup-env.ts && pnpm prisma:generate && prisma migrate deploy",
    "dev:server": "tsx watch --env-file=.env src/server/index.ts",
    "e2e": "playwright test",
    "e2e:server": "DATABASE_URL=file:./prisma/e2e.db PORT=5100 JWT_SECRET=e2e-test-secret-key-12345 NODE_ENV=test concurrently \"tsx src/server/index.ts\" \"PORT=5100 vite --port 5101\"",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:headed": "playwright test --headed",
    "e2e:codegen": "playwright codegen http://localhost:5101",
    "e2e:report": "playwright show-report playwright-report",
    "e2e:install": "playwright install chromium firefox",
    "generate:icon": "tsx scripts/generate-icon.ts",
    "inngest": "node scripts/start-inngest.js",
    "lint": "eslint .",
    "prepublishOnly": "pnpm build",
    "preview": "vite preview",
    "prisma:generate": "PRISMA_HIDE_UPDATE_MESSAGE=true prisma generate --no-hints",
    "prisma:migrate": "prisma migrate dev",
    "prisma:reset": "rm -f prisma/dev.db && prisma generate --no-hints && prisma migrate dev",
    "prisma:seed": "tsx prisma/seed.ts",
    "prisma:seed:hide:projects": "tsx prisma/seed-hide-projects.ts",
    "prisma:seed:workflows": "tsx prisma/seed-workflows.ts",
    "prisma:seed:demo": "tsx prisma/seed-demo-workflow.ts",
    "prisma:studio": "prisma studio",
    "ship": "pnpm build && pnpm version patch && (git commit -am \"bump version\" || true) && pnpm publish",
    "start": "node scripts/start.js",
    "test": "vitest run",
    "test:clean": "rm -f test-worker-*.db*",
    "test:ui": "vitest --ui",
    "test:watch": "vitest"
  },
